<table class="table-details">
  <tr>
    <td>User</td>
    <td>Ranges</td>
    
  </tr>
  <%users.forEach(user =>{ %>
  <tr>
    <td><%= user.username %></td>
    <td><% user.ranges.forEach(range =>{ %>
      <%=range.name + " | " %>
      <%}) %>
    </td>
    <td>
      <a href="users/edit/<%= user.id %>"><button class="btn-form-small">Edit</button></a>
    </td>
    <td>
      <form action="users/ban/<%= user.id %>" method="POST">
        <input type="number" name="banTime">
        <a href=""><button type="submit" class="btn-form-small">BAN</button></a>
      </form>
    </td>
    <td>
      <form action="users/unban/<%= user.id %>" method="POST">
        <a href=""><button type="submit" class="btn-form-small">UNBAN</button></a>
      </form>
    </td>
  </tr>
  
  <% }) %>
</table>