<%-include("../partials/create-btn", {href: "chatrooms/create", btnName:"Create room"})%>
<table class="table-details">
  <tr>
    <td>Chat rooms</td>
    <td>Ranges</td>
  </tr>
  <%chatRooms.forEach(room =>{ %>
  <tr>
    <td><%= room.name %></td>
    <% room.availableRanges.forEach(range => { %>
    <td><%= range.name + " | " %></td>
    <% }) %>
    <td>
      <a href="chatrooms/edit/<%= room.id %>"><button class="btn-form-small">Edit</button></a>
    </td>
  </tr>
  
  <% }) %>
</table>