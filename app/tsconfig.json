{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@/middlewares/*": ["middlewares/*"],
      "@/utils/*": ["utils/*"],
      "@/models/*": ["models/*"],
      "@/services/*": ["services/*"],
      "@/config/*": ["config/*"],
      "@/libs.global/*": ["../../libs/*"]
    },
    "typeRoots": ["@types"],
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
